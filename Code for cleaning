   "source": [
    "customer_summary = transactions.groupby(\"Customer_ID\").agg(\n",
    "    Total_Spend=(\"Transaction_Amount\", \"sum\"),\n",
    "    Avg_Transaction_Value=(\"Transaction_Amount\", \"mean\"),\n",
    "    Transaction_Count=(\"Transaction_Amount\", \"count\")\n",
    ").reset_index()\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
   "id": "88ef2f78-0273-4835-810d-71f17938a676",
   "metadata": {},
   "outputs": [],
   "source": [
    "final_data = customer.merge(\n",
    "    customer_summary,\n",
    "    on=\"Customer_ID\",\n",
    "    how=\"left\"\n",
    ")\n"
   ]
